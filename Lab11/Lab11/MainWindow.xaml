<Window x:Class="Lab11.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lab11"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="Button_Decode" Content="Decode" HorizontalAlignment="Left" Margin="69,183,0,0" VerticalAlignment="Top" MinHeight="30" MaxHeight="30" MinWidth="100" MaxWidth="100" Click="Button_Decode_Click"/>
        <Button x:Name="Button_Save" Content="Save" HorizontalAlignment="Left" Margin="624,183,0,0" VerticalAlignment="Top" MinHeight="30" MaxHeight="30" MinWidth="100" MaxWidth="100" Click="Button_Save_Click"/>
        <TextBox x:Name="TextBox_Hash" Margin="10,10,10,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="120"/>
        <TextBox x:Name="TextBox_Key" Margin="267,165,266,188" TextWrapping="Wrap"/>
        <TextBox x:Name="TextBox_Text" Margin="10,254,10,30" TextWrapping="Wrap" IsReadOnly="True"/>
    </Grid>
</Window>
