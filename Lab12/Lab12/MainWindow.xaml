        <ComboBox x:Name="ComboBox_Animal" SelectedIndex="0" HorizontalAlignment="Center" Margin="0,105,0,0" VerticalAlignment="Top" MaxHeight="60" MinHeight="60" MaxWidth="200" MinWidth="200" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"/>
        <ComboBox x:Name="ComboBox_Board" SelectedIndex="0" HorizontalAlignment="Center" Margin="0,170,0,0" VerticalAlignment="Top" MaxHeight="60" MinHeight="60" MaxWidth="200" MinWidth="200" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"/>
