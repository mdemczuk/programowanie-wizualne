<Window x:Class="Lab05.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lab05"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="button_Load" Content="Wczytaj sekwencję" HorizontalAlignment="Left" Margin="78,55,0,0" VerticalAlignment="Top" Width="109" Height="32" Click="button_Load_Click"/>
        <Button x:Name="button_Find" Content="Znajdź wzorce" HorizontalAlignment="Left" Margin="78,92,0,0" VerticalAlignment="Top" Width="109" Height="32" Click="button_Find_Click"/>
        <ComboBox x:Name="combobox_Choose" HorizontalAlignment="Left" Margin="78,129,0,0" VerticalAlignment="Top" Width="109" Height="32" SelectionChanged="combobox_Choose_SelectionChanged"/>
        <TextBox x:Name="textbox_Found" HorizontalAlignment="Left" Height="187" Margin="45,196,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="190" IsReadOnly="True"/>
        <TextBox x:Name="textbox_Sequence" HorizontalAlignment="Left" Height="342" Margin="294,41,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="469" IsReadOnly="True"/>

    </Grid>
</Window>
